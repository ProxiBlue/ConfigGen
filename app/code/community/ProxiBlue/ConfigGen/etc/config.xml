<?xml version="1.0"?>
<config>
    <modules>
        <ProxiBlue_ConfigGen>
            <version>1.0.0</version>
            <depends>
                <Mage_Core/>
            </depends>
        </ProxiBlue_ConfigGen>
    </modules>
    <global>
        <helpers>
            <configgen>
                <class>ProxiBlue_ConfigGen_Helper</class>
            </configgen>
        </helpers>
        <blocks>
            <configgen>
                <class>ProxiBlue_ConfigGen_Block</class>
            </configgen>
        </blocks>
        <models>
            <configgen>
                <class>ProxiBlue_ConfigGen_Model</class>
            </configgen>
        </models>
    </global>
    <adminhtml>
        <layout>
            <updates>
                <configgen>
                    <file>proxiblue_configgen.xml</file>
                </configgen>
            </updates>
        </layout>
    </adminhtml>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <ProxiBlue_ConfigGen after="Mage_Adminhtml">ProxiBlue_ConfigGen</ProxiBlue_ConfigGen>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
</config> 